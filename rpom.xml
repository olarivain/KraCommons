<project>
	<name>Kra Foundation iOS library</name>
	<groupId>org.kra.commons</groupId>
	<version>1.0.12</version>
	<scm>git</scm>
	
	<artifacts>
		<artifact>
			<artifactId>kra-commons-ios</artifactId>
			<name>KraCommons-iOS</name>
			<silenceXcode>true</silenceXcode>
			<target>
				<name>KraCommons-iPhone-Universal</name>
				<configuration>Release</configuration>
				<buildFolder>Universal</buildFolder>
			</target>
		</artifact>
    <artifact>
			<artifactId>kra-commons-macos</artifactId>
			<name>KraCommons-MacOS</name>
			<silenceXcode>true</silenceXcode>
			<target>
				<name>KraCommons-x86</name>
				<configuration>Release</configuration>
				<buildFolder>Release</buildFolder>
			</target>
		</artifact>
	</artifacts>
     
</project>